<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/onBoardFragment">

    <fragment
        android:id="@+id/onBoardFragment"
        android:name="com.example.hw_41.ui.fragments.onboards.OnBoardFragment"
        android:label="OnBoard"
        tools:layout="@layout/fragment_on_board">
        <action
            android:id="@+id/action_onBoardFragment_to_noteListFragment"
            app:destination="@id/noteListFragment" />
    </fragment>

    <fragment
        android:id="@+id/noteListFragment"
        android:name="com.example.hw_41.ui.fragments.note.ListNoteFragment"
        android:label="All Notes"
        tools:layout="@layout/fragment_note_list">
        <action
            android:id="@+id/action_noteListFragment_to_addNoteFragment"
            app:destination="@id/addNoteFragment" />
    </fragment>

    <fragment
        android:id="@+id/addNoteFragment"
        android:name="com.example.hw_41.ui.fragments.note.AddNoteFragment"
        android:label="Add Note"
        tools:layout="@layout/fragment_note_add">
        <argument android:name="noteId"
            app:argType="integer"/>
    </fragment>

</navigation>